function largestOfFour(arr) {
  var newArray = [];
  var large=0;
  for (var i = 0, max = arr.length;i < max; i+=1) {
    for (var j = 0, max2 = arr[i].length;j < max2; j+=1) {
      if (arr[i][j] > large) 
        large = arr[i][j];
    }
    newArray.push(large);
    large = 0;
  }
  // You can do this!
  return newArray;
}
largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]], "");
